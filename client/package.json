{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives",
    "lint:fix": "eslint . --ext js,jsx --report-unused-disable-directives --fix",
    "format": "prettier-eslint --write \"**/*{.json,.js,.jsx,.ts,.tsx}\" && tsc --noEmit",
    "preview": "vite preview",
    "prepare": "cd .. && husky client/.husky",
    "test": "vitest",
    "test:ui": "vitest --ui --coverage.enabled=true",
    "coverage": "vitest run --coverage",
    "knip": "knip"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/roboto": "^5.0.12",
    "@monaco-editor/react": "^4.6.0",
    "@mui/icons-material": "^5.15.15",
    "@mui/lab": "^5.0.0-alpha.170",
    "@mui/material": "^5.15.15",
    "@mui/styled-engine-sc": "^6.0.0-alpha.18",
    "@testing-library/user-event": "^14.5.2",
    "axios": "^1.6.8",
    "dompurify": "^3.1.0",
    "emmet-monaco-es": "^5.3.2",
    "monaco-editor": "^0.47.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "socket.io-client": "^4.7.5",
    "styled-components": "^6.1.8",
    "zustand": "^4.5.4",
    "zustymiddlewarets": "^1.4.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^15.0.7",
    "@types/dompurify": "^3.0.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jsdom": "^24.1.0",
    "knip": "^5.27.0",
    "prettier": "^3.2.5",
    "prettier-eslint-cli": "^8.0.1",
    "simple-zustand-devtools": "^1.1.0",
    "ts-jest": "^29.2.4",
    "typescript": "^5.5.4",
    "vite": "^5.2.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  }
